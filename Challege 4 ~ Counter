var superCounter = function (str){
var numberLibrary = []
var count = 0;
for (var i = 1; i <= str.length; i++){

if (str.charAt(i) == " "){
count ++;
}}
numberLibrary.push(count+1)
numberLibrary.push(count)
var str = "count me in";
var n = str.length
numberLibrary.push(n)
numberLibrary.push((n-count) / (count+1))

console.log(numberLibrary)}
superCounter("count me in")
