var Char = {};
var string = "";
Char.switch = function(arg) {
 for (var i=0; i < arg.length; i++) {
   string+=(arg[i] === arg[i].toLowerCase() ? arg[i].toUpperCase() : arg[i].toLowerCase());}console.log(string); 
}; 


Char.switch("Life is 10% what happens to you, and 90% of how you REACT to it");
